{
  "general": {
    "checkpointing": {
      "enabled": true
    }
  },
  "mcpServers": {
    "perplexity-ask": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "PERPLEXITY_API_KEY",
        "mcp/perplexity-ask"
      ],
      "env": {
        "PERPLEXITY_API_KEY": "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"
      }
    },
    "neo4j-cypher": {
      "command": "uvx",
      "args": [
        "--with",
        "fastmcp<2.11.4",
        "mcp-neo4j-cypher@0.5.1",
        "--transport",
        "stdio"
      ],
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USERNAME": "neo4j",
        "NEO4J_PASSWORD": "password123",
        "NEO4J_DATABASE": "neo4j"
      }
    },
    "neo4j-memory": {
      "command": "uvx",
      "args": ["--with", "fastmcp<2.11.4", "mcp-neo4j-memory@0.4.3"],
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USERNAME": "neo4j",
        "NEO4J_PASSWORD": "password123",
        "NEO4J_DATABASE": "neo4j"
      }
    },
    "claude-ipc": {
      "command": "uvx",
      "args": ["--from", "/Users/tttv/.claude-ipc-mcp", "claude-ipc-mcp"]
    }
  }
}
